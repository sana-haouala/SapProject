{
	"_version": "1.8.0",
	"sap.app": {
		"id": "nextgen.nextgen",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "nextgen.nextgen.view.Login",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "nextgen.nextgen.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "nextgen.nextgen.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"transition": "slide"
			},
			"routes": [{
				"name": "RouteAcceuil",
				"pattern": "RouteAcceuil",
				"target": ["TargetAcceuil"]
			}, {
				"name": "Requester",
				"pattern": "Requester",
				"target": ["Requester"]
			}, {
				"name": "ListTest",
				"pattern": "ListTest",
				"titleTarget": "",
				"greedy": false,
				"target": ["ListTest"]
			}, {
				"name": "Master",
				"pattern": "Master",
				"titleTarget": "",
				"greedy": false,
				"target": ["Master"]
			},
			{
				"name": "ControlManager",
				"pattern": "ControlManager",
				"titleTarget": "",
				"greedy": false,
				"target": ["ControlManager"]
			}],
			"targets": {
				"Requester": {
					"viewType": "XML",
					"viewName": "Requester",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "nextgen.nextgen.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"transition": "slide"
				},
				"AddReq": {
					"viewType": "XML",
					"viewName": "AddReq",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "nextgen.nextgen.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"transition": "slide"
				},
				"AddR": {
					"viewType": "XML",
					"viewName": "AddR"
				},
				"ListTest": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "listTest",
					"viewPath": "nextgen.nextgen.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"Master": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "master",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"viewPath": "nextgen.nextgen.view"
				},
					"ControlManager": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ControlManager",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"viewPath": "nextgen.nextgen.view"
				}
			}
		}
	}
}